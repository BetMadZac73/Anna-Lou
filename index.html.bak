    <div id="myGallery" class="parallax" style="background-image: url('art/display.jpg'); height: 999px;">
      <div class="title"> Picture Gallery 
        </div>
      <script>
        var x = 7;
        var i;
        var y = 0;
        var side = "left"
        var text = "right"
        for (i = 1; i < x; i++) {
          y = i % 2;
          if ( y == 1 ) {
            side = "left";
            text = "right";
          } else {
            side = "right";
            text = "left";
          }
        document.write('<div class="clearfix '+side+'"');
        document.write('<h3 class="'+side+'"> &nbsp; Image '+i+' </h3>');
        document.write('<img class="gallery '+text+'" src="./art/img_'+i+'.jpg"> </div>');
